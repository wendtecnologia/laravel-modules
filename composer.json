{
    "name": "wend/laravel-modules",
    "type": "library",
    "description": "Laravel library to work with modules",
    "keywords": ["wend", "laravel", "modules"],
    "license": "MIT",
    "authors": [
        {
            "name": "Walker de Alencar"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "composer/installers": "^1.0",
        "wend/laravel-modules-installer-plugin": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Wend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wend\\Test\\": "test/Wend/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.com:wend/laravel-modules-installer-plugin.git"
        }
    ]

}
